class Student:
    def __init__(self, name, grade):
        self.name = name
        self.grade = grade

    def passed(self):
        if self.grade >= 75:
            print(f"{self.name} passed!")
        else:
            print(f"{self.name} failed.")



student1 = Student("Alice", 82)
student2 = Student("John", 60)


student1.passed()
student2.passed()
